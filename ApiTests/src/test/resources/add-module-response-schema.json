{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "_id": { "type": "integer" },
        "name": { "type": "string" },
        "questions": {
          "type": "array",
          "items": { "type": "integer" }
        },
        "quizExamSize": { "type": "integer" },
        "user": { "type": "integer" },
        "blocks": {
          "type": "array",
          "items": { "type": "object" }
        },
        "quizes": {
          "type": "array",
          "items": { "type": "object" }
        },
        "cd": { "type": "string", "format": "date-time" }
      },
      "required": ["_id", "name", "questions", "quizExamSize", "user", "blocks", "quizes", "cd"]
    }
  },
  "required": ["data"]
}